<p id="notice"><%= notice %></p>

<p>
  <b>Format:</b>
  <%= @import.format %>
</p>

<p>
  <b>Input file:</b>
  <%= link_to 'Input File', @import.input_file.url %>
</p>

<p>
  <b>Error file:</b>
  <% if @import.error_file.url.blank? %>
		<%= "No errors" %>
	<% else %>
		<%= link_to 'Error File', @import.error_file.url %>
	<% end %>
</p>

<p>
  <b>Import date:</b>
  <%= @import.import_date %>
</p>

<p>
  <b>Status:</b>
  <%= @import.status %>
</p>

<%= link_to 'Back', imports_path %>
