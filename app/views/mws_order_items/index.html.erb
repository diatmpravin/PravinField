<h1>Listing mws_order_items</h1>

<table>
  <tr>
    <th>Asin</th>
    <th>Amazon order item</th>
    <th>Seller sku</th>
    <th>Title</th>
    <th>Quantity ordered</th>
    <th>Quantity shipped</th>
    <th>Item price</th>
    <th>Item price currency</th>
    <th>Shipping price</th>
    <th>Shipping price currency</th>
    <th>Gift price</th>
    <th>Gift price currency</th>
    <th>Item tax</th>
    <th>Item tax currency</th>
    <th>Shipping tax</th>
    <th>Shipping tax currency</th>
    <th>Gift tax</th>
    <th>Gift tax currency</th>
    <th>Shipping discount</th>
    <th>Shipping discount currency</th>
    <th>Promotion discount</th>
    <th>Promotion discount currency</th>
    <th>Gift wrap level</th>
    <th>Gift message text</th>
    <th>Mws order</th>
    <th>Amazon order</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @mws_order_items.each do |mws_order_item| %>
  <tr>
    <td><%= mws_order_item.asin %></td>
    <td><%= mws_order_item.amazon_order_item_id %></td>
    <td><%= mws_order_item.seller_sku %></td>
    <td><%= mws_order_item.title %></td>
    <td><%= mws_order_item.quantity_ordered %></td>
    <td><%= mws_order_item.quantity_shipped %></td>
    <td><%= mws_order_item.item_price %></td>
    <td><%= mws_order_item.item_price_currency %></td>
    <td><%= mws_order_item.shipping_price %></td>
    <td><%= mws_order_item.shipping_price_currency %></td>
    <td><%= mws_order_item.gift_price %></td>
    <td><%= mws_order_item.gift_price_currency %></td>
    <td><%= mws_order_item.item_tax %></td>
    <td><%= mws_order_item.item_tax_currency %></td>
    <td><%= mws_order_item.shipping_tax %></td>
    <td><%= mws_order_item.shipping_tax_currency %></td>
    <td><%= mws_order_item.gift_tax %></td>
    <td><%= mws_order_item.gift_tax_currency %></td>
    <td><%= mws_order_item.shipping_discount %></td>
    <td><%= mws_order_item.shipping_discount_currency %></td>
    <td><%= mws_order_item.promotion_discount %></td>
    <td><%= mws_order_item.promotion_discount_currency %></td>
    <td><%= mws_order_item.gift_wrap_level %></td>
    <td><%= mws_order_item.gift_message_text %></td>
    <td><%= mws_order_item.mws_order_id %></td>
    <td><%= mws_order_item.amazon_order_id %></td>
    <td><%= link_to 'Show', mws_order_item %></td>
    <td><%= link_to 'Edit', edit_mws_order_item_path(mws_order_item) %></td>
    <td><%= link_to 'Destroy', mws_order_item, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

